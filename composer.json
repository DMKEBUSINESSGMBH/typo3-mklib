{
    "name": "dmk/mklib",
    "description": "Utilities for extensions.",
    "version": "3.0.12",
    "type": "typo3-cms-extension",
    "keywords": [
        "TYPO3 CMS",
        "mvc"
    ],
    "homepage": "http://www.dmk-ebusiness.de/",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "DMK E-BUSINESS GmbH",
            "email": "dev@dmk-ebusiness.de",
            "role": "Developer"
        }
    ],
    "support": {
        "email": "dev@dmk-ebusiness.de",
        "source": "https://github.com/DMKEBUSINESSGMBH/typo3-mklib",
        "issues": "https://github.com/DMKEBUSINESSGMBH/typo3-mklib"
    },
    "replace": {
        "mklib": "self.version",
        "typo3-ter/mklib": "self.version"
    },
    "require": {
        "typo3/cms-core": "^8.7 || ^9.5",
        "typo3/cms-scheduler": "^8.7 || ^9.5",
        "digedag/rn-base": ">=1.10.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "~2@stable"
    },
    "autoload": {
        "classmap": [
            "scheduler/",
            "exception/",
            "interface/",
            "model/",
            "util/",
            "validator/",
            "Classes/",
            "srv/",
            "action/",
            "abstract/",
            "cli/",
            "filter/",
            "marker/",
            "interface/",
            "mod1/",
            "search/",
            "soap/",
            "tca/",
            "view/",
            "repository/"
        ]
    },
    "config": {
        "vendor-dir": ".Build/vendor",
        "bin-dir": ".Build/bin",
        "preferred-install": {
            "typo3/cms": "source"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "mkdir -p .Build/Web/typo3conf/ext/",
            "[ -L .Build/Web/typo3conf/ext/mklib ] || ln -snvf ../../../../. .Build/Web/typo3conf/ext/mklib"
        ]
    },
    "extra": {
        "typo3/cms": {
            "cms-package-dir": "{$vendor-dir}/typo3/cms",
            "web-dir": ".Build/Web"
        }
    }
}
